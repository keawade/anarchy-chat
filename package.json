{
  "name": "anarchy-chat",
  "version": "0.1.0",
  "description": "Anarchic chat built to learn.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "check": "npm run check:build && npm run check:eslint && npm run check:prettier && npm run check:dockerfile",
    "check:build": "tsc --noEmit",
    "check:eslint": "eslint",
    "check:prettier": "prettier --check src",
    "check:dockerfile": "dockerfilelint ./Dockerfile",
    "fix": "npm run fix:eslint && npm run fix:prettier",
    "fix:eslint": "eslint --fix",
    "fix:prettier": "prettier --write src",
    "start": "node --inspect=5858 -r ts-node/register ./src/index.ts",
    "start:watch": "nodemon",
    "start:prod": "node ./dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Keith Wade",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/node": "^12.12.54",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "dockerfilelint": "^1.5.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.2.5",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
